{
  "name": "migrations-lib",
  "version": "0.1.0",
  "author": "Simple Organization",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "mocha tests/**/*.test.ts -r esbuild-register -extension ts",
    "build": "tsc && rollup -c"
  },
  "main": "src/index.ts",
  "dependencies": {
    "better-sqlite3": "^8.4.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.0",
    "chai": "^4.3.7",
    "chokidar": "^3.5.3",
    "esbuild": "^0.18.13",
    "esbuild-register": "^3.4.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.6"
  },
  "type": "module",
  "types": "dist/migrateBetterSQLite3.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": "./dist/migrateBetterSQLite3.js"
    },
    "./migrateBetterSQLite3": {
      "default": "./dist/migrateBetterSQLite3.js"
    },
    "./generateMigrations": {
      "default": "./dist/generateMigrations.js"
    }
  },
  "files": [
    "migrateBetterSQLite3.d.ts",
    "generateMigrations.d.ts",
    "dist/migrateBetterSQLite3.d.ts",
    "dist/generateMigrations.d.ts",
    "dist/migrateBetterSQLite3.js",
    "dist/generateMigrations.js"
